alias vim="nvim"
alias diff='diff --color=auto'
alias ag="ag --path-to-ignore $HOME/.config/ag/ignore"
alias ssh="kitty +kitten ssh"
alias tmux="tmux -f ~/.config/tmux/tmux.conf"
alias emu="$ANDROID_HOME/emulator/emulator"
alias mitmproxy="mitmproxy --set confdir=$HOME/.config/mitmproxy"
alias mitmdump="mitmdump --set confdir=$HOME/.config/mitmproxy"
alias mitmweb="mitmweb --set confdir=$HOME/.config/mitmproxy"
alias adbscreenshot="adb shell screencap -p"
alias comp="docker-compose"
alias xkb="setxkbmap -option caps:super"
alias bspwmrc="$HOME/.config/bspwm/bspwmrc"
alias vnc_sariel="ssh -fL 9900:localhost:5900 sariel sleep 10; vncviewer localhost:9900"
